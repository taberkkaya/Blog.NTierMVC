@model List<ArticleDto>

<div class="col-lg-12">
    <div class="sidebar-item recent-posts">
        <div class="sidebar-heading">
            <h2>Son Eklenenler</En></h2>
        </div>
        <div class="content">
            <ul>
                @foreach (var item in Model)
                {
                    <li>
                        <a href="post-details.html">
                            <h5>@item.Title</h5>
                            <span>@item.CreatedDate.ToString("d MMM dddd HH:mm")</span>
                        </a>
                    </li>
                }
            </ul>
        </div>
    </div>
</div>